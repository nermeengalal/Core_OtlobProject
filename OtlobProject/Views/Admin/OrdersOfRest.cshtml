@model IEnumerable<OtlobProject.ModelViews.innerorder>
@{
    ViewData["Title"] = "OrdersOfRest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h1 style="text-align:center;font-size:30px ;color: #3d9970;font-family: cursive;">Order No : @ViewBag.ordid</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Quantity
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Meal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
      
            </td>
        </tr>
}

    </tbody>
</table>
<div>
    @Html.ActionLink("Back", "OrderList",null,htmlAttributes: new { @class="btn btn-secondary" }) 
</div>
